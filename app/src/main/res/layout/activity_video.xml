<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Video" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="413dp"
            android:layout_height="81dp"
            android:background="?attr/colorPrimary"
            android:backgroundTint="@color/white"
            android:elevation="4dp"
            app:contentInsetStart="30dp"
            app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
            app:title="Video"
            app:titleTextColor="@color/blue"
            tools:layout_editor_absoluteX="-2dp"
            tools:layout_editor_absoluteY="-4dp" />


        <ImageView
            android:id="@+id/imagevideo"
            android:layout_width="match_parent"
            android:layout_height="177dp"
            android:scaleType="centerCrop"
            android:src="@drawable/videolele"
            tools:layout_editor_absoluteX="24dp"
            tools:layout_editor_absoluteY="184dp" />


        <TextView
            android:id="@+id/judulartikel"
            android:layout_width="45dp"
            android:layout_height="19dp"
            android:text="Admin"
            android:textColor="@color/blue"
            android:textSize="13sp"
            android:textStyle="normal"
            tools:layout_editor_absoluteX="24dp"
            tools:layout_editor_absoluteY="369dp" />

        <TextView
            android:id="@+id/judulartikel"
            android:layout_width="118dp"
            android:layout_height="16dp"
            android:text="15 September 2023"
            android:textColor="@color/black"
            android:textSize="13sp"
            android:textStyle="normal"
            tools:layout_editor_absoluteX="24dp"
            tools:layout_editor_absoluteY="388dp" />

        <TextView
            android:id="@+id/judulartikel"
            android:layout_width="334dp"
            android:layout_height="77dp"
            android:text="Cara Budidaya Lele Yang Baik Tanpa Pakan Pabrikan Bikin Sukses Untung Berlipat-lipat"
            android:textColor="@color/black"
            android:textSize="20sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="24dp"
            tools:layout_editor_absoluteY="95dp" />

    </LinearLayout>



</androidx.constraintlayout.widget.ConstraintLayout>